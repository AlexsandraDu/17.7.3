депозит = []
per_cent = {'ТКБ': 5,6, 'СКБ': 5,9, 'ВТБ': 4,28, 'СБЕР': 4,0}
деньги = int(input("Введите сумму:"))
депозит.append(int(per_cent['ТКБ']*деньги/100))
депозит.append(int(per_cent['СКБ']*деньги/100))
депозит.append(int(per_cent['ВТБ']*деньги/100))
депозит.append(int(per_cent['СБЕР']*деньги/100))
печать (депозит)
print("Максимальная сумма, которую вы можете заработать —",max(депозит))
